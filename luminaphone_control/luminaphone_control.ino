String note = "";
int c;

void setup() {
  pinMode(4, INPUT_PULLUP);
  pinMode(5, INPUT_PULLUP);
  pinMode(6, INPUT_PULLUP);
  pinMode(7, INPUT_PULLUP);
  pinMode(8, INPUT_PULLUP);
  pinMode(9, INPUT_PULLUP);
  pinMode(10, INPUT_PULLUP);
  pinMode(11, INPUT_PULLUP);

  pinMode(12, OUTPUT);
}

void loop() {
  if (digitalRead(4) == LOW) { // E3
    note = "";
  } else if (digitalRead(5) == LOW) { //G3
    note = "";
  } else if (digitalRead(6) == LOW) { // A3
    note = "";
  } else if (digitalRead(7) == LOW) { // A3/B3
    note = "";
  } else if (digitalRead(8) == LOW) { // B3
    note = "";
  } else if (digitalRead(9) == LOW) { // C4
    note = "";
  } else if (digitalRead(10) == LOW) { // E4
    note = "";
  } else if (digitalRead(11) == LOW) { // G4
    note = "";
  } else {
    note = "";
  }

  c = note.length();
  for (int x=0; x<c; x++) {
    if (note[x] == '1') {
      digitalWrite(12, HIGH);
    } else if (note[x] == '0') {
      digitalWrite(12, LOW);
    }
  }

}
